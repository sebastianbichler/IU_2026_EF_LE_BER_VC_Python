   ],
   "source": [
    "# weight = 73.0\n",
    "# height = 1.82\n",
    "\n",
    "weight = float(input(\"Please enter your weight (in kg): \"))\n",
    "height = float(input(\"Please enter your height (in m): \"))\n",
    "\n",
    "bmi = weight / (height**2)\n",
    "print(\"BMI:\", bmi)\n",
    "print(\"BMI:\", float(bmi))\n",
    "print(\"BMI:\", int(bmi))"
   ]
